{
  "expo": {
    "name": "SYBA",
    "slug": "syba",
    "version": "0.1.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "syba",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E99537"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.syba.finance",
      "infoPlist": {
        "NSFaceIDUsageDescription": "\uc0dd\uccb4 \uc778\uc99d\uc73c\ub85c \uc571\uc5d0 \ub85c\uadf8\uc778\ud569\ub2c8\ub2e4"
      }
    },
    "android": {
      "package": "com.syba.finance",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E99537"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "\uc0dd\uccb4 \uc778\uc99d\uc73c\ub85c \uc571\uc5d0 \ub85c\uadf8\uc778\ud569\ub2c8\ub2e4"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e5c44edb-cc18-4d85-9c1a-f5fe410998a1"
      }
    },
    "owner": "asadoconkimchi"
  }
}